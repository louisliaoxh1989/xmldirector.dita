\par
\pard\intbl\li800\sl266
{\plain\f2\fs18\cf2
Chapter 5
}
\par
\pard\intbl\keepn\sb132\sl266
{\plain\f0\fs20\b\cf2
If 
}
{\plain\f2\fs18\b\cf2
xref-auto-text='text'
}
\par
\pard\intbl\li800\sl266
{\plain\f2\fs18\cf2
Installation
}
\par
\pard\intbl\keepn\sb132\sl266
{\plain\f0\fs20\b\cf2
If 
}
{\plain\f2\fs18\b\cf2
xref-auto-text='number text'
}
\par
\pard\intbl\li800\sl266
{\plain\f2\fs18\cf2
Chapter 5. Installation
}
\par
\pard\intbl\sb132\sl266
